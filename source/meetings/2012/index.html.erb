<%
  meeting_pages = sitemap
                    .where(:status => 'Published', :category => "meeting")
                    .order_by(:published_at => :desc)
                    .limit(5)
                    .all
                    .select { |page| page.path.include? '/2012/' }
%>
<%= partial 'meetings/meeting_list', locals: { meeting_pages: meeting_pages } %>